{
  "name": "omochi-css",
  "version": "1.2.2",
  "main": "omochi.ts",
  "repository": "git@github.com:kiyotd/omochi-css.git",
  "author": "kiyotd <torigoedesign@gmail.com>",
  "license": "MIT",
  "scripts": {
    "omochi:generate:scss:hot": "bun --hot src/omochi.ts",
    "omochi:generate:scss:watch": "bun --watch src/omochi.ts",
    "omochi:generate:scss": "bun src/omochi.ts",
    "omochi:generate:css": "npm-run-all omochi:generate:css:*",
    "omochi:generate:css:expanded": "sass scss/omochi.scss dist/omochi.css --no-source-map --style=expanded",
    "omochi:generate:css:compressed": "sass scss/omochi.scss dist/omochi.min.css --no-source-map --style=compressed"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "npm-run-all": "^4.1.5",
    "rfs": "^10.0.0",
    "sass": "^1.69.7"
  }
}
